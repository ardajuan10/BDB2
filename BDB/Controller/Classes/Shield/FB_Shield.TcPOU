    <Declaration><![CDATA[FUNCTION_BLOCK FB_Shield
VAR_INPUT
END_VAR
VAR_OUTPUT
	Axis_ready					:BOOL:=FALSE;
END_VAR
VAR

ShieldServo: 		FB_AxisControl;

_Shield_param				:recipe;

distance1:					LREAL:=67;




ToolPositioningSpeed	: LREAL := 20;
ToolPositioningAccel	: LREAL := 100;
ToolPositioningJerk		: LREAL := 1000;
ToolHomingSpeed			: LREAL := 2;

ShieldServoEnable:  BOOL;
ShieldServoEnabled: BOOL;

_ExecutedHome				:BOOL:=FALSE;
_ExecutedStage2 			:BOOL:=FALSE;
_ExecutedStage3				:BOOL:=FALSE;
_ExecutedOffset				:BOOL:=FALSE;

tickcounter_shield: 		INT :=0;
State: 						INT:= 0;
StateNext: 					INT:= 0;

END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
