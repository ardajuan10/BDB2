    <Method Name="_force_home" Id="{da3a4eb7-a6d0-4967-9e09-625b0cedb37e}">
      <Declaration><![CDATA[METHOD _force_home  : BOOL
VAR_INPUT
	Enable				:BOOL;
	
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF enable AND NOT axis_ready THEN
	CASE State OF
	
	0:	
	
		ShieldServo.fPosition:= 0;
		ShieldServo.fVelocity:= 10; //mm/s
	
		ShieldServo.xMoveA:=TRUE;
		StateNext:=5;
	5:	
		IF ShieldServo.xInPosition = TRUE THEN
		
			ShieldServo.xMoveA := FALSE;
			StateNext:=0;
			axis_ready:=TRUE;
		
		END_IF


	END_CASE

	State:=StateNext;
ELSIF enable = FALSE THEN
	axis_ready:=FALSE;
	ShieldServo.xMoveA := FALSE;
	state:=0;
END_IF]]></ST>
      </Implementation>
    </Method>
