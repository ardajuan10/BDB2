    <Method Name="_Home" Id="{e3a2bcf1-6b36-4915-aa6d-4a43f630c251}">
      <Declaration><![CDATA[METHOD _Home 
VAR_INPUT
	
END_VAR


VAR



END_VAR

VAR_OUTPUT
	
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[




CASE State OF //you are here



0:	
	_ExecutedHome := FALSE;
	ShieldServo.fVelocity := ToolPositioningSpeed;
	ShieldServo.fAxisAcceleration := ToolPositioningAccel;
	ShieldServo.fAxisDeceleration := ToolPositioningAccel;
	ShieldServo.fAxisJerk := ToolPositioningJerk;

	IF ixShieldHomeSwitch THEN
		ShieldServo.xPositiveDirection := FALSE;
		IF  tickcounter_shield < 10 THEN
			tickcounter_shield := tickcounter_shield + 1;  // can be put in onditional
		
		ELSE
		//IF tickcounter_compress >= 10 THEN
			ShieldServo.xMoveV := TRUE;
		END_IF
	ELSE
	
		//CompressServo.xHalt := NOT CompressServo.xHalt;
		ShieldServo.xMoveV := FALSE;
		//CompressServo.xHalt := NOT CompressServo.xHalt;
		ShieldServo.xHalt := TRUE;
		IF ShieldServo.xHalted THEN
			tickcounter_shield:=0;
			StateNext := 5;
		END_IF
	
	END_IF
	
5:
	
	ShieldServo.xHalt := FALSE;
	tickcounter_shield := tickcounter_shield + 1;
	IF tickcounter_shield >= 10 THEN
		tickcounter_shield := 0;
		StateNext := 10;
	END_IF
	
10:
	ShieldServo.fVelocity := ToolHomingSpeed;
	
	IF NOT ixShieldHomeSwitch THEN
		ShieldServo.xPositiveDirection := TRUE;
		IF tickcounter_shield < 10 THEN
			tickcounter_shield := tickcounter_shield + 1;
		ELSE
		//IF tickcounter >= 10 THEN
			shieldServo.xMoveV := TRUE;
		END_IF
	ELSE
		shieldServo.xMoveV := FALSE;
		shieldServo.xHalt := TRUE;
		IF shieldServo.xHalted AND shieldservo.fstandstill THEN
			tickcounter_shield := 0;
			StateNext := 15;
		END_IF
	END_IF

15:
	
	shieldServo.xHalt := FALSE;
	tickcounter_shield := tickcounter_shield + 1;
	IF tickcounter_shield >= 25 THEN
		tickcounter_shield := 0;
		StateNext := 20;
		//Executed := TRUE;
	END_IF
20:
	shieldServo.fHomeOffset := 0;
	shieldServo.xZero := TRUE;
	StateNext := 25;	
	
25:
	IF shieldServo.xZeroed THEN
		shieldServo.xZero := FALSE;
		_ExecutedHome := TRUE;
		StateNext := 0;
	END_IF
	

END_CASE

State := StateNext;]]></ST>
      </Implementation>
    </Method>
