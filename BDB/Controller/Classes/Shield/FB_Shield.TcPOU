    <Method Name="_absolute_move" Id="{812a1d69-4259-448f-9f71-0b18ef866d53}">
      <Declaration><![CDATA[METHOD _absolute_move : BOOL
VAR_INPUT
	Enable				:BOOL;
	Stage				:INT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Enable AND NOT Axis_ready THEN
	CASE State OF
	
	0:	
		
		ShieldServo.fPosition:= _Shield_Param.Shield.Distance[Stage];
		ShieldServo.fVelocity:= _Shield_Param.Shield.Speed[Stage]; //mm/s
	
	
		ShieldServo.xMoveA:=TRUE;
	
	
		StateNext:=5;
	5:	
		IF ShieldServo.xInPosition = TRUE THEN
		
			ShieldServo.xMoveA := FALSE;
			StateNext:=0;
			axis_ready:=TRUE;
		
		
		END_IF


	END_CASE

	State:=StateNext;
ELSIF NOT enable THEN
	axis_ready:=FALSE;
	ShieldServo.xMoveA := FALSE;
	state:=0;
END_IF]]></ST>
      </Implementation>
    </Method>
