    <Method Name="Offset" Id="{ac5cd27c-8097-45c7-959a-7f6c2239b74f}">
      <Declaration><![CDATA[METHOD Offset
VAR_INPUT
	Enable				:BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF enable AND NOT axis_ready THEN	
	CASE State OF
	0:	
		_ExecutedOffset:=FALSE;
		ShieldServo.fPosition:= _Shield_Param.Other.Shield_Home_Offset-PRG_Machine.rec_old.other.shield_home_offset;
		ShieldServo.fVelocity:= 20; //mm/s
	
		IF ShieldServo.fPosition<>0 THEN
			ShieldServo.xMoveA:=TRUE;
			StateNext:=5;
		ELSE
			_ExecutedOffset:=TRUE;
		END_IF
	
		//StateNext:=5;
	5:	
		IF ShieldServo.xInPosition = TRUE AND ShieldServo.fstandstill THEN
			
			ShieldServo.xMoveA := FALSE;
			StateNext:=7;
			//_ExecutedOffset:=TRUE;
		
		END_IF
		
	7:
		tickcounter_shield:=tickcounter_shield+1;
		IF tickcounter_shield >= 25 THEN
			tickcounter_shield := 0;
			StateNext := 10;
			//Executed := TRUE;
		END_IF
		
	10:
	
		ShieldServo.fHomeOffset := 0;
		ShieldServo.xZero := TRUE;
		StateNext := 15;	
	
	15:
		IF shieldServo.xZeroed THEN
			shieldServo.xZero := FALSE;
			_ExecutedOffset:=TRUE;
			axis_ready:=TRUE;
			StateNext := 0;
		END_IF

	END_CASE

	State:=StateNext;
	
ELSIF NOT enable THEN 
	axis_ready:=FALSE;
	ShieldServo.xZero := FALSE;
	
	StateNext := 0;
END_IF]]></ST>
      </Implementation>
    </Method>
